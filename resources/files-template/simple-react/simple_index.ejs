import React, { Component } from "react"
import {
  
    
   
    
    
    
    
    StyleSheet,
    
    Text

} from 'react-native';
import { TabNavigator,StackNavigator,TabBarBottom } from 'react-navigation';

import {PlusOperatePage,PlusItemNavigator,PlusItemIcon} from 'uhutu-plus';

import top_support from "../../../code/top/support/top_support";
import current_support from "../../../code/current/support/current_support";

 
import VersionCheck from "../../../native-plug/code-push/version_check";

import styles from "../../../pages/air/resource/layout-style";



<% paths.forEach((fItem)=>{%>
<%- fItem.importName %><% }) %>
//初始化程序


const ScreenTab = TabNavigator({


  <% out.tabItems.forEach((fTabItem)=>{ %>
  <%= fTabItem.tabName %>: {
    screen: <%= fTabItem.screenName %>,
    navigationOptions : {
     
      tabBarIcon: ({ bFlagForce,tintColor }) => (
       <PlusItemIcon pName='<%= fTabItem.iconName %>' style={[styles.layout_tab_normal_icon,{color:tintColor}]} />
      ),
   
  }
  },
  <% }) %>
  
}, {
  tabBarOptions: {
    <%- out.tabOptions.join('\n') %>
    
  },
  tabBarPosition:'bottom',
  tabBarComponent:TabBarBottom,
  animationEnabled:false,
  swipeEnabled:false,
});

const RouteStack={
<% paths.forEach((fItem)=>{%>
<%- fItem.uqName %>:{name:"<%- fItem.uqName %>",screen:<%- fItem.uqName %>},<% }) %>
root_layout_main:{name:"root_layout_main",screen:ScreenTab}

};







const ScreenMain = StackNavigator(RouteStack,{initialRouteName:'root_layout_main',

navigationOptions:{
  headerTitleStyle:styles.layout_nav_title,
  headerStyle:styles.layout_nav_header,
  headerTintColor:'#ffffff',
  headerBackTitle:' ',
  headerBackTitleStyle:styles.layout_nav_back
}
});




export  default class  extends Component{

  constructor(props){
    super(props);
    VersionCheck.checkUpdate(this);
    current_support.initApplication(this);
  }

  
    
  render() {
    return (
      <ScreenMain />
    );
  }
}


