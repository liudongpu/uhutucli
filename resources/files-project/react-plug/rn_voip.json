{
  "desc":"调用融云的相关代码，调用之前需要确保本地装了pod"
  ,
  "ios":[
    {
      "exec":"iosInitPod",
      "set":{

        "contentInfo":[
          "pod 'RongCloudIM/IMLib', '2.8.8'",
          "pod 'RongCloudIM/IMKit', '2.8.8'",
          "pod 'RongCloudIM/CallLib', '2.8.8'",
          "pod 'RongCloudIM/CallKit', '2.8.8'"
        ],
        "name":"react_note_plug_rn_voip"

      }
    },
    {
      "exec":"baseFileOption",
      "set":{
        "optType":3,
        "key":"[@config:define.resourcePath]/files-native/rn-voip/voip_react.js",
        "value":"[@config:appReact.buildPath]/native-plug/voip/voip_react.js"

      }
    },
    {
      "exec":"baseFileOption",
      "set":{
        "optType":3,
        "key":"[@config:define.resourcePath]/files-native/rn-voip/VoipReact.h",
        "value":"[@config:appReact.workPath]/ios/VoipReact.h"

      }
    },
    {
      "exec":"baseFileOption",
      "set":{
        "optType":3,
        "key":"[@config:define.resourcePath]/files-native/rn-voip/VoipReact.m",
        "value":"[@config:appReact.workPath]/ios/VoipReact.m"

      }
    }
  ]

}
