{

  
  "android":
  [
    
  ],
  "ios":
  [
    
    
    {
      "exec": "baseFileOption",
      "set": {
        "optType": 153305,
        "filePath": "[@config:define.devPath]/ufiles/ios-logo/",
        "targetPath": "[@config:file.reactIosWork]/[@config:appReact.workName]/Images.xcassets/AppIcon.appiconset/"

      }
    },
    {
      "exec": "baseFileOption",
      "set": {
        "optType": 150306,
        "filePath": "[@config:define.devPath]/ufiles/ios-logo/Contents.json",
        "targetPath": "[@config:file.reactIosWork]/[@config:appReact.workName]/Images.xcassets/AppIcon.appiconset/Contents.json"

      }
    },
    {
      "exec": "baseContentReplace",
      "set": {

        "filePath": "[@config:file.reactIosWork]/[@config:appReact.workName]/Base.lproj/LaunchScreen.xib",
        "withText": "[@config:project.displayName]",
        "replaceText": ["[@config:appReact.workName]"]
      }
    }
    ,
    {
      "exec": "baseContentReplace",
      "set": {

        "filePath": "[@config:file.reactIosWork]/[@config:appReact.workName]/Base.lproj/LaunchScreen.xib",
        "withText": "Powered by www.yinxl.com",
        "replaceText": ["Powered by React Native"]
      }
    }
  ]

}
